{
    "name": "djavue/tenancy",
    "description": "Djavue Tenancy.",
    "keywords": ["djavue", "tenancy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nam Dinh",
            "email": "dinhtrongnam@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "laravel/framework": ">=7.0"
    },
	"autoload": {
        "psr-4": {
            "Djavue\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Djavue\\Tenancy\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "djavue": {
            "providers": [
                "Djavue\\Tenancy\\TenancyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
